                              <img class="like-image" id="like-image1"
                                src ="/com"/>
                            </div>
                            
                            
                         </div>
                         <a href="/Borrarpost/<%= data[i].ID%>" class="btn btn-danger">Delete</a>
                      </tr>
                    <% } %>
                  <% } %>
                </tbody>
              </table>
        </section>

  </section>

